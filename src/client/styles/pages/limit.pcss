@import "../font.pcss";

body {
    padding: 0;
    margin: 0;
}

#container {
    width: 100%;
    height: 100vh;

    overflow: hidden;

    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;

    background-image: url("../../assets/images/bg.png");

    color: rgba(255, 255, 255, 0.6);

    font-size: 60px;

    div > div {
        margin-bottom: 50px;
    }
}
